[loggers]
keys = root, script

[handlers]
keys = console, script

[formatters]
keys = generic, script

[logger_root]
level = INFO
handlers = console
qualname =

[logger_script]
level = INFO
handlers = script
propagate = 0
qualname = script

[handler_console]
class = logging.StreamHandler
formatter = generic
args = (sys.stderr,)

[handler_script]
class = logging.StreamHandler
formatter = script
args = (sys.stderr,)

[formatter_generic]
format = %(asctime)-15s %(levelname)-5.5s [%(filename)s:%(lineno)s] [%(funcName)s] %(message)s
class = logging.Formatter

[formatter_script]
format = %(message)s
class = logging.Formatter
